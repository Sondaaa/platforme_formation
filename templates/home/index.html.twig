          {% else %}
            <img class="img-fluid" src="{{ asset('assets/img/placeholder.png') }}" alt="Placeholder" />
          {% endif %}
        </a>

        <div class="portfolio-caption">
          <div class="portfolio-caption-heading">{{ formation.titre }}</div>
          <div class="portfolio-caption-subheading text-muted">{{ formation.objectifs|default("Pas d'objectif") }}</div>
        </div>

